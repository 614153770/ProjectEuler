/*tabs*/
.agent-view-main-tabs {
    position: fixed;
    top: 70px;
    right: 0;
    bottom: 0;
    left: 0;

    margin: 0;
}

.agent-view-main-tabs .panel-heading {
    line-height: 40px;

    height: 40px;
