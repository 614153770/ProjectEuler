/*404*/
.wrap-404{
   position: fixed;
   left:50%;
   top:50%;
   background-color: #fff;
   -webkit-border-radius:4px;
   -moz-border-radius:4px;
   border-radius: 4px;
   -webkit-box-shadow: 0 0 30px rgba(0,0,0,.05); 
   -moz-box-shadow: 0 0 30px rgba(0,0,0,.05); 
   box-shadow: 0 0 30px rgba(0,0,0,.05); 
   width:62.5%;
   max-width: 900px;
   height:500px; 
   margin-left:-31.2%;
   margin-top:-250px;
   text-align: center;
   padding-top: 55px;
   background-image: url(../images/404-bg.png);
   background-size: 100% auto;
   border:1px solid #d1d1d1\9; 
   border:1px solid transparent;
   
}
.wrap-404 .text-404{
    font-size:140px;
    font-weight: bold;    
    background: -webkit-gradient(linear, 0 0, 0 80%, from(#5cbff4), to(#318de7));
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    color: #3a98ea;

}
.wrap-404 .not-found-text{
    font-size: 18px;
    color:#333;
    font-weight: 500;
}
.wrap-404 .not-found-reason{
    font-size: 16px;
    color: #999;
    font-weight: 400;
     margin:10px 0 40px;
}


