/*顶部导航*/
.top-nav-bar {
    position: fixed;
    z-index: 99;
    right: 0;
    left: 0;
}

.top-nav-bar .container-fluid {
    padding-right: 0;
    border-bottom: 1px solid #d9d9d9;
    height:70px;
    padding-left: 0px;
    -webkit-box-shadow: 0 0px 8px rgba(0, 0, 0, .15);
    -moz-box-shadow: 0 0px 8px rgba(0, 0, 0, .15);
    box-shadow: 0 0px 8px rgba(0, 0, 0, .15);
}

.top-nav-bar .navbar-header {
    float: left;

    padding: 10px 0;
}

.top-nav-bar .navbar-brand {
    line-height: 50px;
    padding-left:0px;
    height: 50px;
}

.top-nav-bar .navbar-brand > img {
    display: inline-block;
}

.top-nav-bar .nav .nav-item {
    float: left;

    padding: 10px 0;
}

.top-nav-bar .nav > li > a {
    line-height: 50px;

    width: 50px;
    height: 50px;
    margin-left: 10px;
    padding: 0;

    text-align: center;

    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    border-radius: 2px;
    background-color: #f5f5f5;
}

.top-nav-bar .nav .nav-item > a i {
    font-size: 22px;
}

.top-nav-bar .nav .nav-item > a i.md-apps {
    font-size: 24px;
}

.user-info > a {
    width: auto !important;
    padding: 0 10px !important;
    background-color: #fff !important;
    /*width: 165px !important;*/
}

.top-nav-bar .nav .pipe {
    font-size: 36px;
    font-weight: 100;
    line-height: 60px;

    width: 21px;
    height: 60px;
    margin: 0 7px;

    color: #f5f5f5;
}

.top-nav-bar .container-fluid > .navbar-collapse {
    margin: 0;
}

.top-nav-bar .navbar .navbar-collapse.in,
.top-nav-bar .navbar .collapsing {
    clear: left;
    overflow-y: visible;

    width: 100%;
    padding-bottom: 1rem;

    text-align: left;
}

.top-nav-bar .navbar-collapse.in .nav:not(.nav-inline) .nav-item,
.top-nav-bar .collapsing .nav:not(.nav-inline) .nav-item {
    float: none;

    margin: 0;
}

.top-nav-bar .nav > li > a:hover,
/*.top-nav-bar .nav > li > a:focus,*/
.top-nav-bar .nav .nav-item.current > a,
.top-nav-bar .nav .open > a,
.top-nav-bar .nav .open > a:hover,
.top-nav-bar .nav .open > a:focus {
/*color : #27a3e6;*/
    background-color: #e5e5e5;
}

.top-nav-bar .nav #overOiew-btn.disabled {
    pointer-events: none;
}

.top-nav-bar .nav #overOiew-btn.disabled i {
    color: #999;
}


/*顶部导航搜索框*/
.global-search .input-group {
    line-height: 50px;

    width: 50px;
    height: 50px;

    -webkit-transition: all .3s ease;
    -moz-transition: all .3s ease;
    -ms-transition: all .3s ease;
    -o-transition: all .3s ease;
    transition: all .3s ease;

    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    border-radius: 2px;
    background-color: #f5f5f5;
}

.global-search .input-group:hover {
    background-color: #e5e5e5;
}

.global-search i.md-search {
    font-size: 22px;

    position: absolute;
/*// z-index : - 1;*/
    top: 50%;
    left: 13px;

    margin-top: -11px;

    cursor: text;
    pointer-events: none;
}

.global-search .quick-search-input {
    line-height: 50px;

    display: none;

    width: 50px;
    height: 50px;

    -webkit-transition: all .3s ease;
    -moz-transition: all .3s ease;
    -ms-transition: all .3s ease;
    -o-transition: all .3s ease;
    transition: all .3s ease;
    text-indent: 50px;

    border: 1px solid transparent;
    -webkit-border-radius: 2px 0 0 2px;
    -moz-border-radius: 2px 0 0 2px;
    border-radius: 2px 0 0 2px;
    background-color: transparent;
}

.input-group:hover .quick-search-input {
    display: block;
}

.global-search .search-list {
    position: absolute;
    top: 100%;
    right: 0;
    left: 0;

    margin-top: 10px;

    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    background-color: #fff;
    -webkit-box-shadow: 0 5px 30px rgba(0, 0, 0, .15);
    -moz-box-shadow: 0 5px 30px rgba(0, 0, 0, .15);
    box-shadow: 0 5px 30px rgba(0, 0, 0, .15);
}

.global-search .search-list li a {
    line-height: 50px;

    display: block;

    /*height: 50px;*/
    padding: 0 20px;
}

.global-search .search-list li a:hover {
    background-color: #f5f5f5 !important;
}

.global-search .search-list li i {
    font-size: 24px;

    margin-right: 10px;
}

.global-search .search-list li i.md-code-not-equal {
    color: #d195ff;
}

.global-search .advance-search {
/*visibility : hidden;*/
    display: none;

    width: 50px;

    cursor: pointer;

    border: none;
    /*background-color: #e5e5e5;*/
}

.global-search .advance-search i.md-list {
    font-size: 22px;
}

.global-search .advance-search.active {
    border-color: #27a3e6;
    background-color: #27a3e6;
}

.global-search .advance-search.active i.md-list {
    color: #fff;
}

.global-search .popover {
    margin-top: 25px;

    border-color: transparent;
}

.global-search .popover.bottom .arrow {
    border-bottom-color: #f5f5f5;
}

.global-search-focus .input-group {
    width: 380px;
}

.global-search-focus .quick-search-input {
    display: block;

    width: 340px;

    background-color: #f5f5f5;
}
@media screen and (max-width: 1050px) {
    .global-search-focus .quick-search-input {
        width: 280px;
    }
}

.global-search-focus i.md-search {
    z-index: 1;

    color: #565656;
}

.global-search-focus .input-group-addon {
/*// visibility : visible;*/
    display: table-cell;
    border-left: 1px solid #fff;
}

/*advance-search-popover*/
.advance-search-popover-content {
    width: 520px;
    margin: -9px -15px;
}

.advance-search-popover-content .tab-pane {
