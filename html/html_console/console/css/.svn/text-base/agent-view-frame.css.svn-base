}

.advance-search-popover-body .filter-form {
    overflow-y: auto;

    max-height: 400px;
}

.advance-search-popover-body .filter-form label {
    font-size: 12px;
    font-weight: 500;

    text-transform: capitalize;

    color: #999;
}

.advance-search-popover-body .filter-form label.text-u-c {
    text-transform: uppercase !important;
}

.advance-search-popover-body .filter-form .padder-xs {
    padding: 3px;
}

.advance-search-popover-body .filter-form .w160 {
    width: 160px;
}

.advance-search-popover-body .filter-form .w260 {
    width: 260px;
}

.advance-search-popover-body .filter-form .w100 {
    width: 100px;
}

.advance-search-popover-body .filter-form .w40 {
    width: 40px;
}

.advance-search-popover-body .filter-form .w28 {
    width: 328px;
}

.advance-search-popover-body .filter-form .w80 {
    width: 83px;
}

.advance-search-popover-body .filter-form .w105 {
    width: 105px;
}

.advance-search-popover-body .filter-form .w224 {
    width: 224px;
}

.advance-search-popover-body .filter-form input {
    color: #666;
}

.filter-form .connect-input span:not(:first-child):not(:last-child) input {
    margin-left: -1px;

    border-radius: 0;
}

.filter-form .connect-input span:last-child:not(:first-child),
.filter-form .connect-input span:last-child:not(:first-child) input {
    margin-left: -1px;

    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
}

/*统一-drop 隐藏/显示 动效*/

/*从顶部下来*/
.announce-drop,
.personal-drop,
.customer-result-drop,
.ag-360view-main-wrap,
.calendar-drop {
    position: fixed;
    top: -10%;
    right: 0;
    left: 0;

    visibility: hidden;
    /*overflow: auto;*/
    -o-transition: all .2s ease-out;
    -moz-transition: all .2s ease-out;
    -ms-transition: all .2s ease-out;
    transition: all .2s ease-out;

    opacity: 0;
    filter:alpha(opacity=0);
}


.announce-drop.menu-show,
.personal-drop.menu-show,
.customer-result-drop.menu-show,
.ag-360view-main-wrap.menu-show,
.calendar-drop.menu-show
 {
    top: 70px;
    bottom: 0;

    visibility: visible;

    opacity: 1;
    filter:alpha(opacity=100);
    background-color: #fff;
}

.customer-result-drop.menu-show,
.ag-360view-main-wrap.menu-show{
   background-color: #f5f5f5; 
}

.new-bo-drop,
.user-info-drop,
.agent-view-recent-menu-drop,
.note-drop,
.notifications-drop{
    position: fixed;
    z-index: 99;
    top: 70px;
    visibility: hidden;
    /*overflow: auto;*/
    -o-transition: all .2s ease-out;
    -moz-transition: all .2s ease-out;
    -ms-transition: all .2s ease-out;
    transition: all .2s ease-out;

    opacity: 0;
    filter:alpha(opacity=0);
    background-color: #fff;
}

.user-info-drop.menu-show,
.new-bo-drop.menu-show,
.agent-view-recent-menu-drop.menu-show,
.note-drop.menu-show,
.notifications-drop.menu-show{
    visibility: visible;

    opacity: 1;
    filter:alpha(opacity=100);
}


