    float: left;

    width: 50%;
    height: 50px;

    text-align: center;

    border-bottom: 2px solid #fff;
}

.advance-search-popover-content > ul li i {
    font-size: 20px;

    display: inline-block;

    vertical-align: middle;
}

.advance-search-popover-content > ul li a {
