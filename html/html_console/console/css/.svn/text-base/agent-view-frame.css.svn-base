/*导航菜单*/
.agentview-menu-drop {
    position: fixed;
    z-index: 100;
    top: 75px;
    left: 143px;

    display: none;

    width: 260px;
    height: 347px;

    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    background-color: #fff;
}

.agentview-menu-drop.menu-show {
    display: block;
}

.agentview-menu-drop.over-hidden {
    overflow: hidden;
}

.agentview-menu-drop ul {
    margin-bottom: 0;
}

.agentview-menu-drop ul li {
    height: 70px;
    padding: 15px 20px;

    border-bottom: 1px solid #f2f2f2;
}

.agentview-menu-drop ul li:hover {
    background-color: #f5f5f5;
}

.agentview-menu-drop ul li i {
    font-size: 18px;

    float: left;

    margin-right: 10px;
    padding: 10px;

    color: #fff;
    border-radius: 50%;
}

.agentview-menu-drop ul li p {
    float: left;
    overflow: hidden;

    width: 75%;
    margin-bottom: 0;

    white-space: nowrap;
    text-overflow: ellipsis;
}

.all-menu h3 {
    font-size: 14px;
    line-height: 50px;

    height: 50px;
    margin: 0;

    cursor: pointer;
    text-align: center;

    color: #27a3e6;
    border-bottom: 1px solid #e5e5e5;
}

.all-menu > ul {
    display: none;
}

.all-menu > ul > li {
    line-height: 40px;
}

.all-menu ul li i.md-keyboard-arrow-right {
    font-size: 24px;

    position: absolute;
    right: 2px;

    color: #dbdbdb;
}

.all-menu > ul > li .sub-menu {
    position: absolute;
    top: 49px;
    left: 260px;

    display: none;

    width: 260px;
    padding-left: 5px;
}

.sub-menu > ul > li .sub-menu {
    position: absolute;
    top: 0;
    left: 260px;

    display: none;

    width: 230px;
    padding-left: 5px;
}

.all-menu ul li ul {
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    background-color: #fff;
    -webkit-box-shadow: 0 5px 30px rgba(0, 0, 0, .15);
    -moz-box-shadow: 0 5px 30px rgba(0, 0, 0, .15);
    box-shadow: 0 5px 30px rgba(0, 0, 0, .15);
}

.all-menu > ul > li:hover > .sub-menu {
    display: block;
}

.sub-menu > ul > li:hover > .sub-menu {
    display: block;
}


