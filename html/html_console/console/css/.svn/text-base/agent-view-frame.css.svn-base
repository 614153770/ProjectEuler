.agent-view-main-tabs .nav > li {
    margin-right: 0;

    border-radius: 0;
}

.agent-view-main-tabs .nav > li > a {
    font-size: 14px;
    line-height: 39px;

    display: block;

    height: 39px;
    padding: 0 35px 0 15px;

    text-transform: capitalize;

    color: #999;
    border-radius: 0;
    background-color: #fff;
}

.agent-view-main-tabs .nav > li.agent-view-main-tabs-home > a {
    font-size: 22px;
    padding: 0 25px 0 25px;
    text-align: center;

    border-right: 1px solid rgba(0, 0, 0, .1);
