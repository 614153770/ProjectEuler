<script type="text/javascript">
seajs.use('aries-main',function(){
	 $.cookie($.aries.config.sec.SEC_AUTH_ROLETYPE_KEY, "Operator", {path : '/'});
});
     function queryCustomerOrders(){
	    var url = menu_urls['Orders'];
	    $("#menuId").html("<iframe src='"+ url +"'></iframe>");
	 }
	 
	 function initEnv(){
	    var url = menu_urls['Init'];
	    $("#menuId").html("<iframe src='"+ url +"'></iframe>");
	 }
	 
	 function permission(){
		    var url = menu_urls['Permission'];
		    $("#menuId").html("<iframe src='"+ url +"'></iframe>");
		 }
	 
	 function login(){
		//æ¸…cookie
	    	$.removeCookie($.aries.config.sec.SEC_AUTH_TOKEN_KEY, {path : '/'});
	     var tid = $.cookie($.aries.config.sec.SEC_AUTH_TENANT_KEY);
	     if(tid == null){
	         tid = "21";
	     }
		 window.location = '/console/login.html?tid='+tid;
	}
	 
function wechat(){
	 $.openPopupDiv('aso_qrCode_login','WeChat SignUp','470','',{"showButton":false});
	   
	    $.ajaxPost($.aries.service.aso.genQr,'senceIdPrefix=20',function(data){
	        if(data && data.sceneId){
	        	 $('#loaderOrganize-main').hide();
	             $('#aso_qrCode_login').find('img.qrCode-img').attr('src',data.url);
	        }
	    },function(code,info){
	    	if(undefined==info.hub_value[0]){
	            $.message.error("","Login failed!");
			 }else{
	            $.message.error("","login failed,code:"+info.hub_value[0].code+",info:"+info.hub_value[0].value);
			 }
	    });
}
</script>
