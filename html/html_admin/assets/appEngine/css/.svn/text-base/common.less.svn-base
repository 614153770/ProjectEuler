.btn img{
    width:40px;
}
.btn-sm{
    height: 20px!important;
}
.btn-lg{
    height: 40px!important;
}
.btn-xl{
    height: 50px!important;
}
.btn-default{
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    border-radius: 2px;
    background: @white;
    border-color: #dcdcdc;
}
.btn-line,
.btn-line:link,
.btn-line:visited,
.btn-line:active {
    background: none;
    border: 1px solid #f0f4f7 !important;
    color: @white !important;
    border-radius: 2px;
    height: 40px;
    font-size: 16px;
}
.btn-line:hover{
    background-color: @white;
    border: none;
    color: #0288d1 !important;
}
.btn-primary{
    background: #1a94dc;
    border-radius: 2px;
    padding: 0 20px;
    border: 1px solid #0278b8;
    font-family: robotoregular;
}
.btn-primary:hover{
    background-color: #0089d2;
    border-color: #00679e;
}
.btn-ye,
.btn-ye:link,
.btn-ye:visited,
.btn-ye:hover,
.btn-ye:active{
    background-color: #fdd95d;
    color: #474747 !important;
    border-radius: 2px;
    height: 40px;
    font-size: 16px;
    padding: 0 20px;
    font-family: robotoregular;
    border-color: #dea426;
}
.btn-ye:hover{
    background-image:-webkit-linear-gradient(to top, #fcde6d, #feca28);
    background-image:linear-gradient(to top, #fcde6d, #feca28);
}
.btn.disabled, .btn[disabled], fieldset[disabled] .btn{
    background-color: #f0f0f0;
    color: #e0e0e0;
    cursor: not-allowed;
    border:1px solid #e0e0e0;
}

.btn-blue-line,
.btn-blue-line:link,
.btn-blue-line:visited,
.btn-blue-line:hover,
.btn-blue-line:active{
    background-color: @white;
    color: #0288d1;
