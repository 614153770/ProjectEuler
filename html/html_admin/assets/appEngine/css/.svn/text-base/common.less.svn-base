/*common*/
body{font-family:robotoregular;}
ul,li{
    margin: 0;
}
.md-icon{
    font-size: 20px;
    color: @gray-darker;
}
.btn i.md-icon{
    font-size: 20px;
    float: left;
}
@media (min-width: 320px) {
    .body{font-size: 40px;}
}
@media (min-width: 768px) {
    .container {
        width: 760px; }  }
@media (min-width: 992px) {
    .container {
        width: 960px; } }
@media (min-width: 1200px) {
    body{font-size: 14px;}
    .container {width: 960px;}
}

html,body{
    -webkit-text-size-adjust:none;
}
.toggle.disabled{
    opacity: 0.4;
    cursor: not-allowed;
    pointer-events: none;
}
::-webkit-input-placeholder { /* WebKit browsers */
    color:    @gray-light;
    font-style: italic;
}
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
    color:    @gray-light;
    font-style: italic;
}
::-moz-placeholder { /* Mozilla Firefox 19+ */
    color:    @gray-light;
    font-style: italic;
}
:-ms-input-placeholder { /* Internet Explorer 10+ */
    color:    @gray-light;
    font-style: italic;
}
.btn-group .btn{
    border-radius: 0;
}
.dropdown-menu{
    min-width:inherit;
    border-radius: 0;
    border-color: @border-gray;
}
.dropdown-menu>li>a{
    border-radius: 0;
    margin: 0 1px;
}
.arrow, .arrow:after {
    position: absolute;
    display: block;
    width: 0;
    height: 0;
    border-color: transparent;
    border-style: solid;
}
.arrow {
    border-width: 8px;
    left: 60px;
    margin-left: -11px;
    border-top-width: 0;
    border-bottom-color: @border-gray;
    top: -8px;
}
.arrow:after {
    border-width: 7px;
    content: " ";
    top: 1px;
    margin-left: -7px;
    border-top-width: 0;
    border-bottom-color: #fff;
}

.loading{
    transform:scale(0.5);
}

.loading-page{
    width: 100px;
    display: block;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -50px;
    margin-top: -50px;
}

