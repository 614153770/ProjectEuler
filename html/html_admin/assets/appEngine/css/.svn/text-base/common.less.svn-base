/*home-product-list*/
.product-list-title{
    overflow: hidden;
}
.product-list-title h4{
    font-size: 16px;
    border-left:4px solid @border-blue;
    padding-left: 7px;
    margin-left: 5px;
    margin: 25px 0 15px 5px;
    float: left;
}
.product-list-title nav{
    float: right;
}
.product-list-title .pagination .text{
    float: none;
}
.product-list-title .pagination li i{
    color:#bfbfbf
}
.product-list-title .pagination .disabled i{
    color:#e4e4e4
}
.product-list{
    border: 1px solid @border-gray;
    overflow: hidden;
    min-height: 398px;
    position: relative;
    margin-bottom: 10px;
    padding: 0 10px;
}
.product-list:hover{
    .box-shadow(0 5px 20px rgba(0,0,0,0.1));
    transform: translate3d(0, -1px, 0);
    transition: all 0.3s ease;
}



.advert-link:hover,
.advert:hover,
.register:hover{
    .box-shadow(0 5px 20px rgba(0,0,0,0.2));
    transform: translate3d(0, -2px, 0);
    transition: all 0.3s ease;

}
.product-list .btn{
    height: 40px;
}
.product-list-img{
    padding:20px 15px 15px 15px;
    display: table;
    margin: 0 auto;
}
.product-list-img-big{
    float: left;
    padding:48px 20px;
    width: 50%;
    height: 396px;
}

.product-list .info-content{
    float: left;
    text-align: left;
    margin: 115px 0 100px;
    width: 50%;
}
.product-list-img .pic{
    display: table-cell;
    vertical-align: middle;
    height: 198px;
}
.product-list-img .img-responsive{
    max-height: 198px;
}
@-moz-document url-prefix() {
    .product-list-img .img-responsive{
        width: 100%;
    }
}
.product-list-img-big .img-responsive{
    max-height: 300px;
    /*max-width: 200px;*/
    width: auto;
}

.product-list h4{
    font-size: 16px;
    color: @gray-base;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-bottom: 0;
}
.product-list span.info{
    display: block;
    color: @gray-darker;
    font-size:14px;
    width: 100%;
    font-family: robotolight;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    height: 20px;
    cursor:pointer;
}
.product-list span.price{
    display: block;
    color: @gray-dark;
    font-size: 30px;
    padding: 5px 35px 5px 0;
    position: relative;
}
.product-list span.price em{
    font-size: 18px;
    font-style: normal;
    display: inline-block;
    position: absolute;
    top: 14px;
}
.product-list span.price span{
    display: inline-block;
    padding: 0 0 0 10px;
}
.product-list span.price .mo{
    color: @gray-light;
}
.product-list span.compare{
    color: @gray-darker;
    display: block;
    padding: 20px 0 20px 25px;
    cursor: pointer;
    position: relative;
}
.product-list span.compare .vs-circle-plus,
.product-list span.compare .vs-circle-minus{
    font-size: 20px;
    display: inline-block;
    margin-right: 10px;
    position: absolute;
    left: 40px;
    top: 20px;
}
.product-list-vertical{
    min-height: auto;
}
.product-list-vertical .product-list-img .pic{
    height: 120px;
}
.product-list-vertical .product-list-img .img-responsive{
    max-height: 120px;
}
.product-list-vertical span.compare{
    padding: 10px 0 20px 25px;
