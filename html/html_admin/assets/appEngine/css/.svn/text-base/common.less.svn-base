/*toggle*/
.toggle-default .toggle-inner{
    width: 120px;
}

.toggle-default .toggle-slide{
    border-radius: 15px;
    border-color: @border-gray;
}
.toggle-default .toggle-slide .toggle-on{
    background-color: #1a94dc;
    height: 30px;
    width: 54px;
    text-indent: -12px;
    line-height: 30px;
}
.toggle-default .toggle-slide .toggle-blob{
    height: 30px;
    width: 40px;
    margin-left: -12px;
}
.toggle-default .toggle-slide .toggle-inner{
    margin-left: -42px;
}
.toggle-active .toggle-slide{
    border-color: @border-blue;
}
.toggle-default .toggle-slide .toggle-off{
    background-color: #ececec;
    height:30px;
    width: 40px;
    margin-left: -2px;
    text-indent: 2px;
    line-height: 30px;
}
.toggle-active .toggle-slide .toggle-inner{
    margin-left:0;
    transition: all 0.2s ease;
}
.toggle-default .toggle-slide .toggle-off, .toggle-default .toggle-select .toggle-on{
    text-align: center;
}
/*nav-1*/
.main-nav{
    margin:0 auto 10px;
    position: relative;
}
.main-nav h2{
    font-size: 24px;
    font-family: robotolight;
    border-left: 1px solid @border-gray;
    float: left;
    padding: 0 10px;
    margin: 15px 0 0 0;
}
.nav-bar{
    padding: 0;
}
.main-nav-fixed .nav-bar{
    position: fixed;
    top:-4px;
    z-index: 1043;
    background-color:rgba(8,30,47,0.95);
    width: 100%;
    padding: 10px 0 5px;
}
.main-nav .container{
    position: relative;
}
.main-nav-fixed .top-nav{
    opacity: 0;
}

.main-nav .logo{
    float: left;
    margin-right: 10px;
}
.main-nav-fixed .logo{
    width: 56px;
    overflow: hidden;
    transform: scale(0.7);
}
.main-nav-fixed .nav-list{
    font-size: 16px;
}
.main-nav .form-control{
    height: 40px;
    position: absolute;
    right: 0;
    opacity: 0;
    top: 6px;
    z-index: 1;
    width: 0;
    transition: all 0.3s ease;
}
.main-nav-fixed .form-control{
    right: 94px;


}
.main-nav i.search{
    position: relative;
    z-index: 2;
    margin-right: 10px;
    cursor: pointer;
    color: @gray-darker;
    padding-left: 1px;
}
.main-nav-fixed .input-icon > i{
    top: 0px;
}
.main-nav .input-icon{
    /*margin-top: 7px;*/
    /*float: right;*/
    /*width: 100%;*/
}
.top-nav{
    text-align: right;
    color: @gray-lighter;
}
.top-nav a{
    color: @gray-darker;
    padding:10px;
    display: inline-block;
    font-size: 12px;
}
.top-nav a:hover,
.top-nav a:hover i{
    color: @blue-hover;
}
.top-nav a i{
    margin-right: 6px;
    font-size: 12px;
}
.top-nav a.ico{
    position: relative;
}
.top-nav a.ico .icon-arrowlup-down{
    margin-top: 5px;
    display: inline-block;
    float: right;
    margin-left: 3px;
    margin-right: 0;
}
.top-nav a.nav-on{
    border-top: 4px solid @border-blue;
    color: @blue-hover;
}
.nav-list{
    font-size: 18px;
    position: relative;
}
.nav-list ul li{
    margin: 0 0 0 40px;
    line-height: 52px;
    float: left;
}
.nav-list ul li a{
    color:@gray-darker;
}
.nav-list ul li a:hover,
.nav-list ul li a.current{
    color: @blue-hover;
}
.nav-list-search{
    position: absolute;
    width: 0;
    opacity: 0;
}
.nav-list-search.open{
    width: 100%;
    opacity: 1;
}
.nav-list ul li.nav-ico{
    //color:#9e9e9e;
    line-height: 42px;
    font-size: 20px;
    padding-top: 4px;
    display: none;
    margin-left: 0;
}
.main-nav-fixed ul li.nav-ico{
    display: inline-block;
    line-height: 36px;
    margin-top: 5px;
}
.nav-ico > span{
    border-right: 1px solid @border-gray;
    padding: 0 22px;
    position: relative;
    display: inline-block;
    height: 18px;
    cursor: pointer;
    color: @gray-darker;
}
.top-nav a.ico .label-info{
    position: absolute;
    background-color: #fdb82c;
    color: @white;
    width: 30px;
    height: 30px;
    border: none;
    padding: 0;
    line-height: 30px;
    text-align: center;
    font-size: 16px;
    left: 10px;
    top: -5px;
    transform: scale(0.6);
    -webkit-transform: scale(0.6);
}
.top-nav a.ico .label-info em{
    position: absolute;
    top: -7px;
    left: 9px;
    transform: scale(1.5);
    font-style: normal;
}
.nav-ico > span .label-info{
    position: absolute;
    background-color: #fdb82c;
    color: @white;
    width: 18px;
    height: 18px;
    border: none;
    padding: 0;
    line-height: 18px;
    text-align: center;
    font-size: 12px;
    right: 8px;
    top:-10px;
    transform: scale(0.8);
}
.nav-ico > span:hover,
.nav-ico > span.on{
    color:@blue-hover;
}
.nav-ico > span.vs-user-login{
    border: none;
}
.nav-ico > span i{
    position: absolute;
    left: 10px;
    top: 0;
    font-size: 16px;

}
.nav-ico .icon-arrowlup-down{
    position: absolute;
    bottom: -4px;
    font-size: 14px;
    right: 7px;
    transform: scale(0.6);
}


