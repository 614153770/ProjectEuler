.main-ckbox {
    position: relative;
    line-height: 16px;
}

.main-ckbox input[type="checkbox"]{
    opacity: 0;
}

.main-ckbox label {
    padding-left: 10px;
    cursor: pointer;
    margin-bottom: 7px !important;
}

.main-ckbox label:before {
    width: 16px;
    height: 16px;
    position: absolute;
    top:3px;
    left:1px;
    content: '';
    display: inline-block;
    -moz-border-radius: 2px;
    -webkit-border-radius: 2px;
    border-radius: 2px;
    border: 1px solid @border-gray-darker;
    background: @white;
}

.main-ckbox label.disabled{
    color: @gray-light;
    cursor:not-allowed;
}

.main-ckbox label.disabled:before  {
    opacity: 0.4
}
.main-ckbox label.checked:after{
    font-family: 'FontAwesome';
    content: "\F00C";
    position: absolute;
    top:3px;
    left:3px;
    display: inline-block;
    font-size: 11px;
    width: 16px;
    height: 16px;
    color: @white;
}

.main-ckbox label.checked_notall:after{
    content:'';
    position:absolute;
    top:5px;
    left:5px;
    display:inline;
    background:@border-gray-darker;
    width:8px;
    height:8px;
    overflow:hidden;
}
.main-ckbox label.checked_notall:before{
    border-color:@border-gray-darker;
    background-color:@white;
}

.main-ckbox a{
    color: @gray-dark;
}

//.main-ckbox a:hover{
//    color: #bbb;
//}


.main-ckbox input[type="checkbox"]:checked + label::before  {
    border-color: @blue;
    background-color: @blue;
}
.main-ckbox input[type="checkbox"]:checked + label::after  {
    font-family: 'FontAwesome';
    content: "\F00C";
    position: absolute;
    top: 3px;
    left: 3px;
    display: inline-block;
    font-size: 11px;
    width: 16px;
    height: 16px;
    color: @white;
}

